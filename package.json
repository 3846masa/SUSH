  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "validator": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "cssnext": "^1.8.4",
    "eslint": "^2.2.0",
    "fixpack": "^2.3.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^1.5.1",
    "postcss-url": "^5.1.1",
    "precss": "^1.4.0",
    "uglify-save-license": "^0.4.1",
    "webpack": "^1.12.14"
  },
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "bower": "bower --save",
    "build": "npm-run-all -p build:*",
    "build:asset": "ncp src/asset/ dist/asset/",
    "build:css": "postcss -c config/postcss.config.js -d dist/css src/css/*.css",
    "build:js": "webpack --config config/webpack.config.js",
    "fixpack": "fixpack",
    "postinstall": "bower install",
    "serve": "http-server -p 8000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:*",
    "watch:asset": "watch 'npm run build:asset' src/asset/",
    "watch:css": "postcss -w -c config/postcss.config.js -d dist/css src/css/*.css",
    "watch:js": "webpack --config config/webpack.config.js --progress --colors --watch",
    "watch:serve": "npm run serve"
  }
}
