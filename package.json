{
  "devDependencies": {
    "@3846masa/typedoc-default-themes": "https://github.com/3846masa/typedoc-default-themes.git",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@types/google.analytics": "^0.0.33",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/power-assert": "^1.4.29",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.1.8",
    "babili-webpack-plugin": "^0.0.11",
    "codecov": "^2.1.0",
    "codelyzer": "^3.0.1",
    "cpx": "^1.5.0",
    "cz-lerna-changelog": "^1.1.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "lerna": "2.0.0-rc.4",
    "lerna-semantic-release": "^9.0.7",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.2",
    "read-pkg": "^2.0.0",
    "rimraf": "^2.6.0",
    "rxjs": "^5.0.1",
    "testdouble": "^2.1.2",
    "ts-loader": "^2.0.1",
    "tslint": "^5.1.0",
    "tslint-config-valorsoft": "^2.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-espower-loader": "^1.0.1",
    "xhr-mock": "^1.7.0",
    "zone.js": "^0.8.9"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prebuild": "npm run init",
    "build": "npm-run-all build:sush build:plugins",
    "build:sush": "lerna exec --scope sush -- webpack --config ../../webpack.config.js",
    "build:plugins": "lerna exec --scope sush-plugin-* -- webpack --config ../../webpack.config.js",
    "postbuild": "npm run doc",
    "clean": "lerna exec -- rimraf tsconfig.json src/**/*.d.ts dist doc coverage",
    "doc": "lerna exec -- typedoc --out doc/ --mode file --target ES2015 --theme ../../node_modules/@3846masa/typedoc-default-themes/bin/minimal src/index.ts",
    "init": "npm-run-all clean init:config bootstrap",
    "init:config": "lerna exec -- cpx ../../tsconfig.json .",
    "release": "npm run release:pre && npm run release:perform && npm run release:post",
    "release:pre": "lerna-semantic-release pre",
    "release:perform": "lerna-semantic-release perform",
    "release:post": "lerna-semantic-release post",
    "report": "lerna exec -- codecov",
    "pretest": "npm-run-all init build:sush",
    "test": "lerna exec --concurrency 1 -- karma start ../../karma.conf.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  }
}
